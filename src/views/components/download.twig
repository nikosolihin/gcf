<a href="{{data.file}}" class="Download">
  <svg aria-hidden="true" role="presentation" title="">
    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#{{data.icon}}"></use>
  </svg>
  <div class="Download-body">
    <div class="Download-name hregular brown">{{data.filename|title}}</div>
    {% if data.extension %}
      <div class="Download-extension hsmall caps spaced faint">{{data.extension}}</div>
    {% endif %}
  </div>
</a>
